    <section>
      <para style="P4">[[repeatIn(get_payslip_lines(o.line_ids),'p') ]]</para>
      <blockTable colWidths="228.0,118.0,98.0,95.0" style="Table9">
        <tr>
          <td>
            <para style="P12">[[ ('Total' in p.name or 'Liquido' in p.name or removeParentNode('para')) and '' ]][[p.name]]</para>
            <para style="P4">[[('Total' not in p.name and 'Liquido' not in p.name or removeParentNode('para')) and '']] [[p.name]]</para>
          </td>
          <td>
            <para style="P5">[[('Total' not in p.name and 'Liquido' not in p.name or removeParentNode('para')) and '']][[ '%.0f' % p.quantity ]]</para>
            <para style="P5Bold">[[('Total' in p.name or 'Liquido' in p.name or removeParentNode('para')) and '']][[ '%.0f' % p.quantity ]]</para>
          </td>
          <td>
            <para style="P5">[[('Total' not in p.name and 'Liquido' not in p.name or removeParentNode('para')) and '']] [[ formatoNumero(p.amount) ]]</para>
            <para style="P5Bold">[[('Total' in p.name or 'Liquido' in p.name or removeParentNode('para')) and '']] [[ formatoNumero(p.amount) ]]</para>
          </td>
          <td>
            <para style="P5">[[('Total' not in p.name and 'Liquido' not in p.name or removeParentNode('para')) and '']] $ [[ formatoNumero(p.total) ]]</para>
            <para style="P5Bold">[[('Total' in p.name or 'Liquido' in p.name or removeParentNode('para')) and '']] $ [[ formatoNumero(p.total) ]]</para>
          </td>
        </tr>

      </blockTable>
      
    </section>
    <blockTable colWidths="539.0" style="Table14">
