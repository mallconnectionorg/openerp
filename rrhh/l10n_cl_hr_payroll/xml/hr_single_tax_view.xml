<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    
       
			<record id="hr_single_tax_tree_view" model="ir.ui.view">
			<field name="name">hr.single.tax.tree</field>
			<field name="model">hr.single.tax</field>
			<field name="arch" type="xml">
				<tree string="tree_string">
					<field name="period" />
					<field name="from_" />
					<field name="to_" />
					<field name="percent" />
					<field name="reduction" />
				</tree>
			</field>
		</record>

		<record id="hr_single_tax_form_view" model="ir.ui.view">
			<field name="name">hr.single.tax.form</field>
			<field name="model">hr.single.tax</field>
			<field name="priority" eval="8" />
			<field name="arch" type="xml">
				<form string="OpenERP Version 7" version="7.0">
					<header>
						<button name="action_button" string="Button Name" type="object"
							class="oe_highlight" />
					</header>
					<sheet>
						<group colspan="4" col="4">
							<field name="period" />
							<field name="from_" />
							<field name="to_" />
							<field name="percent" />
							<field name="reduction" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_hr_single_tax_tree_view">
			<field name="name">Single tax</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">hr.single.tax</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="context">{}</field>
		</record>
		
		</data>
</openerp>